<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%-4level] %msg%n</pattern>
    </encoder>
  </appender>
  <!-- %logger{36} -->
  <root level="debug">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>